<!DOCTYPE html>
<html ng-app="procesos_pa">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Procesos Intec</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <style>
        .colorBg {
            background-color: #404040;
            color: #FFF;
        }

        .BgColor {
            color: #404040;
            background-color: #FFF;
        }

        .section-title-lyt {
            padding-left: 15px;
            padding-top: 20px;
            font-weight: 100;
            margin: 10px;
        }

        .btn {
            border: 20px;
            font-weight: 100;
            margin-bottom: 20px;
         
        }
    </style>

    @*MEILIO * ANGULAR +*@
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

    @*<script src="~/Scripts/AngularJS/jquery-3.2.1.min.js" type="text/javascript"> </script>*@
    <script src="~/Scripts/AngularJS/angular.min.js" type="text/javascript"> </script>
    <script src="~/Scripts/AngularJS/NgRoute.js" type="text/javascript"> </script>
    <script src="~/Scripts/AngularExecute.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/AddSubjectsController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/AdminSubjectController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/AddTrimesterController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/ProfesorAutorizacionController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/AddSectionController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/RevisionController.js" type="text/javascript"> </script>

    @*EMILIO * ANGULAR -*@




</head>
<body class="colorBg">

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")






    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a href="">
                    <img class="logo" src="~/Content/images/logo_top.png">
                </a>
            </div>
            <div class="navbar-collapse collapse">
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div>
        <h3 class="colorBg section-title-lyt"> Opciones</h3>
    </div>
    <div class="col-md-2 colorBg">
        @Html.ActionLink(" Inicio", "Inicio", "Estudiante", routeValues: null, htmlAttributes: new { @class = "btn colorBg glyphicon glyphicon-home" })<br />
        @Html.ActionLink(" Seleccion", "Seleccion", "Estudiante", routeValues: null, htmlAttributes: new { @class = "btn colorBg glyphicon glyphicon-file" })<br />
        @Html.ActionLink(" Preseleccion", "Preseleccion", "Estudiante", routeValues: null, htmlAttributes: new { @class = "btn colorBg glyphicon glyphicon-file" })<br />
        @Html.ActionLink(" Retiro", "Retiro", "Estudiante", routeValues: null, htmlAttributes: new { @class = "btn colorBg glyphicon glyphicon-share" })<br />
        @Html.ActionLink(" Revision", "Revision", "Estudiante", routeValues: null, htmlAttributes: new { @class = "btn colorBg glyphicon glyphicon glyphicon-edit" })<br />
        @Html.ActionLink(" Oferta Academica", "OfertaAcademica", "Estudiante", routeValues: null, htmlAttributes: new { @class = "btn colorBg glyphicon glyphicon-share" })<br />
    </div>

    <div id="Contenedor">
        <div class="container BgColor col-md-10">
            @RenderBody()
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)





</body>
</html>
