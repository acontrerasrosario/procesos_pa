<!DOCTYPE html>
<html ng-app="procesos_pa">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Procesos Intec</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <style>
        .colorBg {
            color: #ffffff;
            background: #000000
        }

        .BgColor {
            background: #ffffff;
            color: #000000
        }
    </style>

    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

    @*<script src="~/Scripts/AngularJS/jquery-3.2.1.min.js" type="text/javascript"> </script>*@
    <script src="~/Scripts/AngularJS/angular.min.js" type="text/javascript"> </script>
    <script src="~/Scripts/AngularJS/NgRoute.js" type="text/javascript"> </script>
    <script src="~/Scripts/AngularExecute.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/AddSubjectsController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/AdminSubjectController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/AddTrimesterController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/ProfesorAutorizacionController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/AddSectionController.js" type="text/javascript"> </script>
    <script src="~/UI/Controllers/PublicarController.js" type="text/javascript"> </script>


</head>
<body class="colorBg">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a href="">
                    <img class="logo" src="~/Content/images/logo_top.png">
                </a>
            </div>
            <div class="navbar-collapse collapse">
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div>
        <h3 class="colorBg"> Opciones</h3>
    </div>
    <div class="col-md-2 colorBg">
        @Html.ActionLink("Publicar", "Publicar", "Profesor", routeValues: null, htmlAttributes: new { @class = "btn colorBg" })<br />

    </div>

    <div class="col-md-10 BgColor" id="Contenedor">
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer></footer>
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

