<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>Procesos Intec</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <style>
        .navbar{
            color:#fff;            
        }

        .Bg2{
            color:#fff;
            background-color:#444;
            margin-right:inherit;
            align-content:center;            
        }

        .logo{
            vertical-align:middle;
            align-items: flex-start;
        }

        .col-md-10{
            margin-left:10%;
        }

        .w3-sidebar{
            color:#fff;
            background-color:#444;
        }

        .w3-dropdown-hover{
            color:#fff;
            background-color:#444;
        }
        .nav{
            color:#fff;
           background-color:#444;
           vertical-align:middle;
           text-align:center;
        }
        .Menu{
            vertical-align:middle;
            max-width:200px;
        }
        #btn-1{
            color: #fff;
            background-color: #444;
        }
        
        


       
    </style>

</head>
<body class="Body">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">              
                <a href="">
                    <img class="logo" src="~/Content/images/logo_top.png">
                </a>
            </div>

            <div class="navbar-collapse collapse ">                
                @Html.Partial("_LoginPartial")
            </div>

        </div>
    </div>
    <div>
        <h3 class="Bg2"> Opciones</h3>
    </div> 
    
    <div class="Menu">
        <nav>
            <ul class="nav">
                <li>
                    <a href="#" id="btn-1" data-toggle="collapse" data-target="#submenu1" aria-expanded="false">Publicacion</a>
                    <ul class="nav collapse" id="submenu1" role="menu" aria-labelledby="btn-1">
                        <li><a href="#">Publicacion de Mediotermino</a></li>
                        <li><a href="#">Publicacion Final</a></li>
                        <li><a href="#">Publicacion de Incompleto</a></li>
                    </ul>
                </li>
                <li><a href="#">Revision</a></li>

            </ul>
        </nav>
    </div>

    <div>
        <h3 class="Title"></h3>
    </div>

    
    

    <div class="col-md-10 BgColor" id="Contenedor">
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer></footer>
        </div>
    </div>
       
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


</body>
</html>
